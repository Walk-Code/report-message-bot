{
    "name": "lib/report-message-bot",
    "description": "usually report error message",
    "keywords": [
        "redis"
    ],
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-redis":"*",
        "myclabs/php-enum": "^1.6.6",
        "guzzlehttp/guzzle": "^6.5",
        "ext-mbstring": "*",
        "phpmailer/phpmailer": "^6.6"
    },
    "license": "TIM",
    "autoload": {
        "psr-4": {
            "reportMessage\\": "src/reportMessage"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "reportMessageTest\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^3",
        "m6web/redis-mock": "^5.3"

    },
    "scripts": {
        "php-cs-fixer": "php-cs-fixer --config=./.php_cs"
    }
}
